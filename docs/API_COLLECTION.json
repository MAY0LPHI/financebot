{
  "_type": "export",
  "__export_format": 4,
  "resources": [
    {
      "_type": "request_group",
      "name": "FinBot API",
      "description": "Financial Control Bot API Collection",
      "environment": {},
      "requests": [
        {
          "_type": "request",
          "name": "Register",
          "method": "POST",
          "url": "{{ base_url }}/auth/register",
          "body": {
            "mimeType": "application/json",
            "text": "{\n  \"email\": \"test@example.com\",\n  \"password\": \"Test123!\",\n  \"name\": \"Test User\"\n}"
          }
        },
        {
          "_type": "request",
          "name": "Login",
          "method": "POST",
          "url": "{{ base_url }}/auth/login",
          "body": {
            "mimeType": "application/json",
            "text": "{\n  \"email\": \"demo@finbot.test\",\n  \"password\": \"Demo123!\"\n}"
          }
        },
        {
          "_type": "request",
          "name": "Get Profile",
          "method": "GET",
          "url": "{{ base_url }}/auth/profile",
          "headers": [
            {
              "name": "Authorization",
              "value": "Bearer {{ token }}"
            }
          ]
        },
        {
          "_type": "request",
          "name": "List Accounts",
          "method": "GET",
          "url": "{{ base_url }}/accounts",
          "headers": [
            {
              "name": "Authorization",
              "value": "Bearer {{ token }}"
            }
          ]
        },
        {
          "_type": "request",
          "name": "Create Account",
          "method": "POST",
          "url": "{{ base_url }}/accounts",
          "headers": [
            {
              "name": "Authorization",
              "value": "Bearer {{ token }}"
            }
          ],
          "body": {
            "mimeType": "application/json",
            "text": "{\n  \"name\": \"Conta Corrente\",\n  \"type\": \"CHECKING\",\n  \"balance\": 1000,\n  \"currency\": \"BRL\"\n}"
          }
        },
        {
          "_type": "request",
          "name": "List Transactions",
          "method": "GET",
          "url": "{{ base_url }}/transactions",
          "headers": [
            {
              "name": "Authorization",
              "value": "Bearer {{ token }}"
            }
          ]
        },
        {
          "_type": "request",
          "name": "Create Transaction",
          "method": "POST",
          "url": "{{ base_url }}/transactions",
          "headers": [
            {
              "name": "Authorization",
              "value": "Bearer {{ token }}"
            }
          ],
          "body": {
            "mimeType": "application/json",
            "text": "{\n  \"type\": \"EXPENSE\",\n  \"amount\": 50,\n  \"currency\": \"BRL\",\n  \"description\": \"Almoço\",\n  \"date\": \"2024-01-15T12:00:00Z\",\n  \"isPaid\": true\n}"
          }
        },
        {
          "_type": "request",
          "name": "Cash Flow Report",
          "method": "GET",
          "url": "{{ base_url }}/reports/cash-flow?dateFrom=2024-01-01&dateTo=2024-01-31",
          "headers": [
            {
              "name": "Authorization",
              "value": "Bearer {{ token }}"
            }
          ]
        },
        {
          "_type": "request",
          "name": "Chat Bot",
          "method": "POST",
          "url": "{{ base_url }}/chat",
          "headers": [
            {
              "name": "Authorization",
              "value": "Bearer {{ token }}"
            }
          ],
          "body": {
            "mimeType": "application/json",
            "text": "{\n  \"message\": \"Qual é o meu saldo?\"\n}"
          }
        }
      ]
    }
  ],
  "environments": [
    {
      "_type": "environment",
      "name": "Development",
      "data": {
        "base_url": "http://localhost:3001",
        "token": ""
      }
    },
    {
      "_type": "environment",
      "name": "Production",
      "data": {
        "base_url": "https://api.finbot.example.com",
        "token": ""
      }
    }
  ]
}
